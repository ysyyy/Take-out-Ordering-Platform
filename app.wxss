/**app.wxss**/
.container {
  background-color: #fff;
  height: 100%;
  display: flex;
  flex-direction: column;
  padding: 50rpx 0 0 0;
  box-sizing: border-box;
} 
.about-banner{
  width:750rpx;
  height:550rpx;
}
.about-word{
  font-style:oblique;
  font-weight:bold;
  font-size:50rpx;
  margin-top:20rpx;
  margin-left:30rpx;
}
.about-price{
  font-family: 'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;
  font-weight: bold;
  font-size:40rpx;
  margin-top:30rpx; 
  margin-left:50rpx;
}
.bottom-bar{
    display:flex;
    position: fixed;
    bottom:0;
    width:100%;
    height:80px;
    line-height:80px;
    background:rgb(53, 50, 50);
    box-shadow: 0 -5px 10px #ccc;
}
.gouwuche{
  background:rgb(53, 50, 50);
  height: 100%;
}
.gouwuche-icon{
  font-size:40px;
  color:#87888E;
  margin-left:10px;
  position: relative;
}
.activity-color{
  color:rgb(243, 220, 8);
}
.number-msg{
  padding: 5px 2px;
  border-radius:50%;
  background:red;
  color:white;
  font-size:14px;
  position: absolute;
  display: flex;
  box-sizing: border-box;
  align-items: center;
  top:10px;
  right:-10px;
  width:20px;
  height: 20px;
}
.gouwuche-price{
  color:#A9A9A9;
  display:flex;
  align-items: center;
  margin-left:10px;
  font-size:15px
}
.bottom-bar.hidden{
  display:none;
}
.bottom-bar.view{
    flex-grow: 1;
}
.bottom-bar .count{
    padding-left:20px;
    display:flex;
}
.bottom-bar .num{
    margin-right:20px;
    font-size:18px;
    font-weight:bolder;
    color:rgb(243, 220, 8);
}
.bottom-bar .btn{
    margin-top: 20px;
    margin-left:70px;
    color:rgb(243, 220, 8);
    line-height:40px;
    font-size:16px;
}
.bottom-bar .blank{
    margin-top: 20px;
    margin-left:10px;
    color:#A9A9A9;
    line-height:40px;
    font-size:16px;
}
.food-numb{
  height: 40rpx;
  width: 130rpx;
  display: flex;
  justify-content: flex-end;
  margin-top:-45rpx; 
  margin-left: 500rpx;
  line-height: 40rpx;
  font-size: 25rpx;
}
.food-numb .add,.food-numb .remove{
  width: 40rpx;
  height: 40rpx;
  border-radius: 40rpx;
  text-align: center; 
}
.food-numb .add{
  font-size: 35rpx;
  line-height: 37rpx;
  font-weight: 700;
  background: rgb(243, 220, 8);
  margin-left: 15rpx;
}
.food-numb .remove{
  margin-right: 15rpx;
  line-height: 28rpx;
  font-size: 60rpx;
  font-weight: 500;
  box-sizing: border-box;
  border: 2rpx;
  border-style: solid; 
  border-color: #B0B0B0;
  color: #B0B0B0;
}
.drawer_screen {  
  width: 100%;  
  height: 100%;  
  position: fixed;  
  top: 0;  
  left: 0;  
  z-index: 1000;  
  background: #000;  
  opacity: 0.5;  
  overflow: hidden;  
}  
.cartlist-float{  
  width: 100%; 
  overflow: hidden;  
  position: fixed;   
  z-index: 1001;
  bottom: 80px;
  max-height: 250px;  
} 
.label-cart-bar{
  color:#878787;
  font-size:12px;
  border-left:4px solid rgb(243, 220, 8);
  margin-left: 20px;
}
.icon-clear{
    color:#878787;
    float: right;
    margin-right: 10px;
}
.label-clear{
    color:#878787;
    font-size: 12px;
}
.cart-scroll-list{
  height:450rpx;
}
.cart-list-box{
  background:#FFFFFF;
  display:flex;
  font-size:13px;
  align-items: center;
}
.list-info{
  width:50%;
  padding:5px 15px;
}
.list-info-size{
  font-size:10px;
  color:#B1B1B1;
}
.icon-li-circle{
  margin-left:15px;
  font-size:20px
}
.font20{
  font-size:20px
}

