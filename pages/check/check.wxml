<!--pages/check/check.wxml-->
<view class='sum'>
  <view class='order-info'>
    <view class='order-info-title'>订单详情,取餐号{{qucanhao}},{{time}}</view>
    <view class='cart-list-box' wx:for="{{cartList}}" wx:key="unique" style='border-bottom:1px #E3E3E3 solid'>
      <view class='list-info'>
        <view>{{item.name}}</view>
      </view>
      <view style='width:50%;padding:10px;'>
        <view class='rightDisplay'>
          <view style='color:#A3A3A3'>x {{item.numb}}</view>
          <view>¥ {{item.totalCos}}.00</view>
        </view>
      </view>
    </view>
  </view>
  <view>
    <text class='order-info-title'>取餐前请截图，并在取餐时将截图给服务员看</text>
  </view>
</view>
