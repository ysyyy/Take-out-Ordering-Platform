<!--pages/paysuccess/paysuccess.wxml-->
<view class="contaner" hidden='{{contanerhidden==1}}'>
  <view class="info">
    <icon type="success" size="80" color="green"/>
    <text>支付成功！</text>
  </view>
  <view>
    <button class='return-button' bindtap='return'>返回</button>
  </view>
  <view>
    <button class='check-button' bindtap='check'>查看订单内容</button>
  </view>

</view>
